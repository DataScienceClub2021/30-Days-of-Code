      "source": [
        "n = 10 #BS\n",
        "swap_number = 0\n",
        "lst = [0,5,9,1,2,3,6,4,|||8|||,9]\n",
        "for i in range(n-1):\n",
        "  for j in range(n-1-i):\n",
        "    if lst[j] > lst[j+1]:\n",
        "      lst[j],lst[j+1] = lst[j+1],lst[j]\n",
        "      swap_number+=1\n",
        "print(lst)\n",
        "print(swap_number)"
      ],
      "execution_count": 10,
